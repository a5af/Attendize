{
    "name": "xybio/equis-attend",
    "description": "Equis EMS forked off Attendize (https://github.com/Attendize/Attendize)",
    "keywords": ["event management", "ticket selling", "tickets", "events"],
    "license": "proprietary",
    "type": "project",
    "homepage" : "https://www.attendize.com",
    "require": {
        "dompdf/dompdf": "dev-master",
        "filp/whoops": "~1.0",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "dev-master",
        "iron-io/iron_mq": "2.*",
        "jenssegers/rollbar": "^1.5",
        "laracasts/utilities": "^2.1",
        "laravel/framework": "5.2.*",
        "laravel/socialite": "~2.0",
        "laravelcollective/html": "~5.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "~2.0.0",
        "maxhoffmann/parsedown-laravel": "dev-master",
        "mews/purifier": "~2.0",
        "milon/barcode": "dev-master",
        "nitmedia/wkhtml2pdf": "dev-master",
        "omnipay/bitpay": "dev-master",
        "omnipay/coinbase": "dev-master",
        "omnipay/common": "~2.3",
        "omnipay/migs": "^2.1",
        "omnipay/paypal": "*",
        "omnipay/stripe": "*",
        "predis/predis": "~1.0",
        "vinelab/http": "dev-master",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~2.4",
        "barryvdh/laravel-ide-helper": "^2.1",
        "doctrine/dbal": "~2.3",
        "fzaninotto/faker": "^1.5",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~6.5",
        "symfony/css-selector": "^3.3",
        "symfony/dom-crawler": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Http/Controllers",
            "app/Models",
            "app/Attendize"
        ],
        "psr-4": {
            "App\\": "app/",
            "Attendize\\": "app/Models"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0"
        }
    }
}
